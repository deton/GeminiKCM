type OVERLAY

# Del
#map key 14  MINUS

#map key 28  DEL
# Enter
map key 28  FUNCTION

# \
map key 40  SEMICOLON

# Shift right
map key 42  APOSTROPHE
# .
map key 52  COMMA
# Up
map key 103 PERIOD
# Shift left
map key 54  SLASH

# Ctrl
map key 29  GRAVE
# Fn
map key 464 MINUS
# Alt
map key 56  CTRL_LEFT
# Space
map key 57  SHIFT_LEFT
# , /
map key 51  SPACE
# Left
map key 105 ENTER
# Down
map key 108 EQUALS
# Right
map key 106 BACKSLASH

### ROW 1

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    fn:                                 '~'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '@'
    fn:                                 '`'
}

key 3 {
