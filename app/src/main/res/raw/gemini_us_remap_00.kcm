key COMMA {
    label:                              ','
    base:                               ','
    shift:                              '<'
    fn:                                 replace PAGE_DOWN
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift:                              '>'
    fn:                                 replace DPAD_UP
}

key SLASH {
    label:                              '/'
    base:                               '/'
    shift:                              '?'
    fn:                                 replace PAGE_UP
}

### ROW 5

key GRAVE {
    label:                              '`'
    base:                               '`'
