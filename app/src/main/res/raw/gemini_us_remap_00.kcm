#    fn:                                 replace ALT_LEFT
#}

#key SHIFT_LEFT {
#}

key SPACE {
    label:                              ' '
    base:                               ' '
    ctrl:                               fallback LANGUAGE_SWITCH
    fn:                                 fallback SPACE
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
    fn:                                 replace DPAD_LEFT
}

key EQUALS {
    label:                              '='
    base:                               '='
    shift:                              '+'
    fn:                                 replace DPAD_DOWN
}

key BACKSLASH {
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
    fn:                                 replace DPAD_RIGHT
}
